// define a function
fun factorial(n)
{
    if (n <= 1)
        return 1;
    else
        return n * factorial(n - 1);
}

// print 5!
print factorial(5);

// calculate and print an expression
x = 20;
y = 33;
print (y-x) * 2 - y + 17;

// for loop
for (i = 0; i < 5; i = i+1)
{
    print i;
    if (i == 2)
        break;
}

// while loop
i = 0;
while (i < 10)
{
    if (i % 2)
    {
        i = i + 1;
        continue;
    }

    print factorial(i);
    i = i + 1;
}
